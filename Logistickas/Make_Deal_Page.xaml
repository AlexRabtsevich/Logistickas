<UserControl x:Class="Logistickas.Make_Deal_Page"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             
             xmlns:local="clr-namespace:Logistickas"
             mc:Ignorable="d" 
             d:DesignHeight="680" d:DesignWidth="1118">
    <Grid>
        <materialDesign:TransitioningContent x:Name="TransitionigContentSlide" OpeningEffect="{materialDesign:TransitionEffect SlideInFromLeft, Duration= 0:0:0.8}">
            <Grid>
                <Grid Background="Beige" Height="100" Width="1118" VerticalAlignment="Top">
                    
                </Grid>
                <Grid Height="120" VerticalAlignment="Bottom" Background="Aquamarine">
                    <Button Height="70" Width="120" x:Name="Button_Next" Click="Button_Next_Click"/>
                </Grid>
                <Grid x:Name="GridMake" Height="460" Margin="0 100 0 120" Background="Brown">
                    
                </Grid>
            </Grid>
        </materialDesign:TransitioningContent>


    </Grid>
</UserControl>
